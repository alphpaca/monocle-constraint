{
  "name": "alphpaca/monocle-constraint",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Jacob Tobiasz",
      "email": "jacob@alphpaca.io"
    }
  ],
  "require": {
    "php": "^8.3",
    "composer/composer": "^2.7",
    "composer/metadata-minifier": "^1.0",
    "composer/semver": "^3.2",
    "composer-plugin-api": "^2.6",
    "symfony/http-client": "^5.4 || ^6.4 || ^7.0",
    "symfony/http-client-contracts": "^2.5 || ^3.5"
  },
  "autoload": {
    "psr-4": {
      "Alphpaca\\Monocle\\Constraint\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Alphpaca\\Monocle\\Constraint\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.x-dev"
    },
    "class": "Alphpaca\\Monocle\\Constraint\\Plugin",
    "plugin-modifies-downloads": true
  }
}
